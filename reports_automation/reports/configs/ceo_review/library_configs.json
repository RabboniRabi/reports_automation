        "brc_merge_config" : {
            "join_on" : ["cols.block_name", "cols.udise_col"],
            "merge_type" : "left"
        },
        "post_process_brc_merge" : false,
        "elementary_report" : {
            "generate_report": true,
            "un_ranked_report_args" : {
                "custom_unranked_report": false,
